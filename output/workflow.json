{
  "name": "Workflow test",
  "nodes": [
    {
      "id": "21f26bff-7275-40a7-a22e-6881792f50b8",
      "name": "Schedule Trigger 1",
      "type": "n8n-nodes-base.scheduleTrigger",
      "parameters": {
        "rule": {
          "interval": [
            {
              "field": "Weeks",
              "weeksInterval": 1,
              "triggerAtDay": [
                0
              ],
              "triggerAtHour": 21
            }
          ]
        }
      }
    },
    {
      "id": "9d159be4-05c4-4d92-bfcd-0d4b500ab2a4",
      "name": "AI Agent",
      "type": "@n8n/n8n-nodes-langchain.agent",
